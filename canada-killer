PlrTab:Button({
	Text = 'Deposit (ALL)',
	Callback = function()
	if Player.Character.Money.Value >= 1 then
		game:GetService("ReplicatedStorage").NATMEVENTERRERERE:FireServer(tostring(Player.Character.Money.Value), "deposit")
		sound()
	end
	end
})
